<h1>Travel Locations in Cape Town</h1>


<% @places.each do |place| %>
  <p>
  <%= link_to place.name, place_path(place.id)%>
    <!--<a href="/places/<%= place.id %> Name:<%= place.name%></a>"-->

  <%= link_to 'Destroy', place_path(place),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  </p>



<% end %>